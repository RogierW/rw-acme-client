  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "psr/log": "^3.0",
    "spatie/laravel-data": "^3.9"
  },
  "require-dev": {
    "larapack/dd": "^1.0"
  },
  "autoload": {
    "psr-4": {
